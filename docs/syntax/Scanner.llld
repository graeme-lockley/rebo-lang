tokens
    Identifier = id {digit | id} ['?' | '!'] {chr(39)};
    LiteralInt = ['-'] digit {digit}';
    LiteralChar = chr(39) (char | '\\' ('\\' | 'n' | char(39))) chr(39);
    LiteralSymbol = ':' id {digit | id};

comments
   "//" {!cr};

whitespace
  chr(0)-' ';

fragments
  digit = '0'-'9';
  digits = digit {digit};
  id = 'A'-'Z' + 'a'-'z' + '_';
  cr = chr(10);
  char = chr(32)-chr(38) + chr(40)-chr(91) + chr(93)-chr(255);
