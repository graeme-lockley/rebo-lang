let range(min = 10, max) = {
  if max == () -> {
    max := min
    min := 0
  }

  let result = []
  while min < max -> {
    result := result + [min]
    min := min + 1
  }

  result
}

let filter(l, f) = {
  let filterLoop(lst) =
    if lst == [] -> []
     | f(lst[0]) -> [lst[0]] + filterLoop(lst[1:])
     | filterLoop(lst[1:])

  filterLoop(l)
}

