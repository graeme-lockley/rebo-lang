let range(min = 10, max) = {
  if max == () -> {
    max := min
    min := 0
  }

  let loop(l, acc) =
    if l >= max -> acc
     | loop(l + 1, acc + [l])       

  loop(min, [])
}

let filter(l, f) = {
  let filterLoop(lst = []) =
    if lst == [] -> []
     | f(lst[0]) -> [lst[0]] + filterLoop(lst[1:])
     | filterLoop(lst[1:])

  filterLoop(l)
}

let prime? = import("prime.rebo").prime?

filter(range(500), prime?)
