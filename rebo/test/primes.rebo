let range(min = 10, max) = {
  if max == () -> {
    max := min
    min := 0
  }

  let result = []
  while min < max -> {
    result := result + [min]
    min := min + 1
  }

  result
}

let filter(l, f) = {
  let result = []
  let lp = 0

  while lp < len(l) -> {
    let item = l[lp]

    if f(item) -> {
      result := result + [item]
    }
    lp := lp + 1
  }

  result
}

let prime? = import("prime.rebo").prime?

let go(n) = filter(range(n), prime?)